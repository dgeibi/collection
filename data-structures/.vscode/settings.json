// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "Iosevka Slab, monospace",

    "editor.fontSize": 17,

    "git.enabled": true,

    "git.autofetch": true,

    "window.zoomLevel": 2.0,

    "editor.cursorStyle": "line",

    "editor.wrappingColumn": 0,

    "editor.tabCompletion": true,

    "editor.tabSize": 2,

    "files.encoding": "gbk"

}
