{
  "name": "watch",
  "version": "1.0.0",
  "description": "stop watch written with redux",
  "main": "index.js",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-macros": "^0.5.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.29.0",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.6.1",
    "wtf-webpack-config": "0.0.1"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --env.production"
  },
  "author": "dgeibi",
  "license": "MIT",
  "dependencies": {
    "emotion": "^7.0.11",
    "performance-now": "^2.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "touch": "^3.1.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "emotion/babel",
        {
          "inline": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "jsx-a11y/href-no-hash": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off",
      "jsx-a11y/no-noninteractive-element-interactions": "off"
    }
  }
}
